<!--The status feed partial.-->
<div class="main-page-bk">
  <div class="back-btn">
    <%= link_to @user do %>
      <%= image_tag('back-btn-2.png', width: 40, height: 20, alt: 'GO BACK') %>
    <% end %>
  </div>
  <% if @feed_items.any? %>
    <ol class="microposts">
      <%= render @feed_items %>
    </ol>
    <%= will_paginate @feed_items %>
  <% else %>
    <%= image_tag('search-not-found.png', class: 'search-not-found')  %>
  <% end %>
</div>
